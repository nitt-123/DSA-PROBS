#include<bits/stdc++.h>
using namespace std;
int factorial(int n){
int fact = 1;
for(int i = 2 ; i<=n; i++){
fact = fact*i;
}
return fact;
}
int main(){
cout<<"enter the number to claculate factorial<<endl;
int n;
cin>>n;
int k = factorial(int n);
cout<<"the factorial of the given number is"<<k<<endl;
return 0;
}
